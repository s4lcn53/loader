repeat task.wait() until game.Players.LocalPlayer and game.Players.LocalPlayer.Character
if not game:IsLoaded() then
    game.Loaded:Wait()
end

print("Supported game!!!!")
local creator = game.CreatorId

local games = {
    [35623634] = 'https://raw.githubusercontent.com/s4lcn53/loader.gitignore.io/refs/heads/main/Games/TypicalRNG.lua', -- typical.RNG
    [33799733] = 'https://raw.githubusercontent.com/s4lcn53/loader.gitignore.io/refs/heads/main/Games/Lootify.lua', -- Lootify
    [35961432] = 'https://raw.githubusercontent.com/s4lcn53/loader.gitignore.io/refs/heads/main/Games/Merge%20a%20brainrot.lua', -- Merge brainrot
    [35961432] = 'https://raw.githubusercontent.com/s4lcn53/loader.gitignore.io/refs/heads/main/Games/Merge%20a%20brainrot.lua', -- Merge Garden
}

if games[creator] then
    print("Delete system 32")
    loadstring(game:HttpGet(games[creator]))()
else
    warn("Unsupported game.")
    task.wait(2)
    game.Players.LocalPlayer:Kick("Unsupported game!")
end
